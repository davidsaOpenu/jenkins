    <hudson.tasks.Shell>
      <command># test simulator-ci change×“
cd ${WORKSPACE}/simulator-ci

# redirect qemu output to /tmp/log-XYZ
# git fetch https://review.gerrithub.io/davidsaOpenu/simulator-ci refs/changes/51/381251/4 &amp;&amp; git cherry-pick FETCH_HEAD

# ansible roles for ELK stack
# git fetch https://review.gerrithub.io/davidsaOpenu/simulator-ci refs/changes/95/381395/1 &amp;&amp; git cherry-pick FETCH_HEAD


cd ${WORKSPACE}/simulator-ci/ansible
set -o pipefail
ansible --version
ansible-playbook  -e &apos;short_mode=true&apos; -i hosts playbooks/run_full_setup.yml -vvvv --extra-vars &quot;dest=${WORKSPACE}/simulator&quot; --extra-vars &quot;workspace=${WORKSPACE}&quot; --extra-vars=@global_vars.yml

# run cppcheck
/opt/cppcheck/cppcheck --enable=all --inconclusive --xml --xml-version=2 ${WORKSPACE}/simulator/eVSSIM/CONFIG  ${WORKSPACE}/simulator/eVSSIM/FTL_SOURCE  ${WORKSPACE}/simulator/eVSSIM/MONITOR  ${WORKSPACE}/simulator/eVSSIM/osc-osd  ${WORKSPACE}/simulator/eVSSIM/ServerSocket  ${WORKSPACE}/simulator/eVSSIM/SSD_MODULE  ${WORKSPACE}/simulator/eVSSIM/tests QEMU/hw/nvme* 2&gt; ${WORKSPACE}/cppcheck.xml

</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}/simulator/eVSSIM/tests/host
/opt/oclint/build/oclint-release/bin/oclint-json-compilation-database \
     -- \
     -report-type pmd \
     -o oclint.xml  \
     -max-priority-1=10000 \
     -max-priority-2=10000 \
     -max-priority-3=10000 \
     -list-enabled-rules \
     -enable-global-analysis \
     -enable-clang-static-analyzer</command>
    </hudson.tasks.Shell>
  </builders>
